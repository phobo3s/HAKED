SELECT TBISLER.ISADI, TBHAKED.HAKEDISNO, TBMALZKALS.URUNKODU, TBMALZKALS.ACIKLAMA, TBBINALAR.BINAADI, TBKATLAR.KATADI, TBBLOKLAR.BLOKADI, TBMETRAJ.[MAHAL/NOTLAR], IIf(IsNull([TBMETRAJ]![MIKTAR]),[TBMETRAJ]![EN]*[TBMETRAJ]![BOY]*[TBMETRAJ]![YUKSEKLIK],[TBMETRAJ]![MIKTAR]) AS MIKTAR, TBMETRAJ.EN, TBMETRAJ.BOY, TBMETRAJ.YUKSEKLIK, TBMETRAJ.PURS, TBMETRAJ.BENZER, TBBIRIMS.BIRIMADI, TBMALZKALS.BIRIMFIYAT, TBPARABIRIMS.PARABIRIMI
FROM TBPARABIRIMS INNER JOIN (TBKATLAR INNER JOIN ((TBFIRMA INNER JOIN TBISLER ON TBFIRMA.Kimlik = TBISLER.FIRMA) INNER JOIN ((TBBIRIMS INNER JOIN TBMALZKALS ON TBBIRIMS.Kimlik = TBMALZKALS.BIRIM) INNER JOIN (TBHAKED INNER JOIN (TBBLOKLAR INNER JOIN (TBBINALAR INNER JOIN TBMETRAJ ON TBBINALAR.Kimlik = TBMETRAJ.BİNA) ON TBBLOKLAR.Kimlik = TBMETRAJ.BLOK) ON TBHAKED.Kimlik = TBMETRAJ.HAKEDISKOD) ON TBMALZKALS.Kimlik = TBMETRAJ.MALZKAL) ON TBISLER.Kimlik = TBHAKED.ISADI) ON TBKATLAR.Kimlik = TBMETRAJ.KAT) ON TBPARABIRIMS.Kimlik = TBMALZKALS.PARABIRIMI
WHERE (((IIf(IsNull(FORMS.FRMAIN.Açılan_Kutu62),'*',FORMS.FRMAIN.Açılan_Kutu62=TBHAKED.ISADI))<>False) And ((IIf(IsNull(FORMS.FRMAIN.Açılan_Kutu78),'*',TBHAKED.HAKEDISNO=FORMS.FRMAIN.Açılan_Kutu78))<>False))
ORDER BY TBHAKED.HAKEDISNO, TBMALZKALS.URUNKODU;

