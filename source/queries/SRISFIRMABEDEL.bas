SELECT TBISLER.Kimlik, TBISLER.ISADI, TBFIRMA.FIRMAADI, TBISLER.SOZLTURU, TBISLER.SOZLNO, TBISLER.SOZLTARIH, TBISLER.YERTESTARIH, TBISLER.ISBITIMTARIH, TBISLER.SUREUZATIMI, TBPARABIRIMS.PARABIRIMI, TBISBEDELS.ANASOZLKESIFBEDELI, TBISBEDELS.ILAVESOZLBEDELI, [TBISBEDELS]![ANASOZLKESIFBEDELI]+[TBISBEDELS]![ILAVESOZLBEDELI] AS TOPLAMSOZLESMEBEDELI, TBISBEDELS.KDV, TBISBEDELS.NAKITTEMINAT AS İfade1, TBISBEDELS.DIGERTEMINAT AS İfade2, TBISBEDELS.DAMGAVERGISI AS İfade3, TBISBEDELS.STOPAJ AS İfade4, TBISBEDELS.KDVTEVKIFATI AS İfade5, TBISBEDELS.ISAVANSI AS İfade6
FROM TBPARABIRIMS INNER JOIN ((TBFIRMA INNER JOIN TBISLER ON TBFIRMA.Kimlik = TBISLER.FIRMA) INNER JOIN TBISBEDELS ON TBISLER.Kimlik = TBISBEDELS.ISKODU) ON TBPARABIRIMS.Kimlik = TBISBEDELS.PARABIRIMI
WHERE (((IIf(IsNull([FORMS].[FRMAIN].[Açılan_Kutu62]),'*',[FORMS].[FRMAIN].[Açılan_Kutu62]=[TBISLER].[Kimlik]))<>False));

