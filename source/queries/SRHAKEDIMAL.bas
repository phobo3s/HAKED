SELECT TBISLER.ISADI, TBHAKED.HAKEDISNO, TBIMALATKALS.POZNO, TBIMALATKALS.ACIKLAMA, TBBINALAR.BINAADI, TBKATLAR.KATADI, TBBLOKLAR.BLOKADI, TBMETRAJ.[MAHAL/NOTLAR], IIf(IsNull([TBMETRAJ]![MIKTAR]),[TBMETRAJ]![EN]*[TBMETRAJ]![BOY]*[TBMETRAJ]![YUKSEKLIK],[TBMETRAJ]![MIKTAR]) AS MIKTAR, TBMETRAJ.EN, TBMETRAJ.BOY, TBMETRAJ.YUKSEKLIK, TBMETRAJ.PURS, TBMETRAJ.BENZER, TBBIRIMS.BIRIMADI, TBIMALATKALS.BIRIMFIYAT, TBPARABIRIMS.PARABIRIMI
FROM TBPARABIRIMS INNER JOIN (TBKATLAR INNER JOIN ((TBFIRMA INNER JOIN TBISLER ON TBFIRMA.Kimlik = TBISLER.FIRMA) INNER JOIN ((TBBIRIMS INNER JOIN TBIMALATKALS ON TBBIRIMS.Kimlik = TBIMALATKALS.BIRIM) INNER JOIN (TBHAKED INNER JOIN (TBBLOKLAR INNER JOIN (TBBINALAR INNER JOIN TBMETRAJ ON TBBINALAR.Kimlik = TBMETRAJ.BÝNA) ON TBBLOKLAR.Kimlik = TBMETRAJ.BLOK) ON TBHAKED.Kimlik = TBMETRAJ.HAKEDISKOD) ON TBIMALATKALS.Kimlik = TBMETRAJ.IMALATKAL) ON TBISLER.Kimlik = TBHAKED.ISADI) ON TBKATLAR.Kimlik = TBMETRAJ.KAT) ON TBPARABIRIMS.Kimlik = TBIMALATKALS.PARABIRIMI
WHERE (((IIf(IsNull([FORMS].[FRMAIN].[Açýlan_Kutu62]),'*',[FORMS].[FRMAIN].[Açýlan_Kutu62]=[TBHAKED].[ISADI]))<>False) AND ((IIf(IsNull([FORMS].[FRMAIN].[Açýlan_Kutu78]),'*',[TBHAKED].[HAKEDISNO]=[FORMS].[FRMAIN].[Açýlan_Kutu78]))<>False))
ORDER BY TBHAKED.HAKEDISNO, TBIMALATKALS.POZNO;

